<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <system.diagnostics>
    <trace>
      <listeners>
        <add name="TextWriter"
          type="System.Diagnostics.TextWriterTraceListener"
          initializeData="log.txt" />
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>